{
  "manifest_version": 3,
  "name": "Key Scraper",
  "description": "Enable scraping keys from game bundle websites such as humble bundle and fanatical.",
  "version": "0.9",
  "icons": {
    "16": "images/keyicon16.png",
    "32": "images/keyicon32.png",
    "64": "images/keyicon64.png",
    "128": "images/keyicon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/keyicon16.png",
      "32": "images/keyicon32.png",
      "64": "images/keyicon64.png",
      "128": "images/keyicon128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting", "storage"]
}
